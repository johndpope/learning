package practice;

public class TreeNode {
	public TreeNode left;
	public TreeNode right;
	public int data;
	public TreeNode(int data) {
		this.data = data;
		this.left = null;
		this.right = null;
	}
}

class BinaryTree {
	TreeNode root = new TreeNode(5);
	root.left = new TreeNode(4);
	root.right = new TreeNode(6);
	root.left.left = new TreeNode(3);
}